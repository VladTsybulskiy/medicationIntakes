{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "screens/*": ["./src/screens/*"],
      "store/*": ["./src/store/*"],
      "utils/*": ["./src/utils/*"]
    }
  }
}
